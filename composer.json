{
    "name": "microffice/units",
    "description": "Microffice's Units managment package.",
    "keywords": ["CRM", "ERP", "microffice", "units"],
    "type": "package",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Stoemp Saucisse",
            "email": "stoempsaucisse@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.8",
        "illuminate/support": "^5.0",
        "illuminate/database": "~5.0",
        "illuminate/console": "~5.0",
        "illuminate/events": "~5.0",
        "illuminate/filesystem": "~5.0",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "behat/behat": "~2.5",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Microffice\\": "src/Microffice/"
        },
        "classmap": [
            "src/commands",
            "src/Microffice/helpers"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Microffice\\Units\\Tests\\": "tests/",
            "": ["tests/migrations", "tests/seeds"]
        }
    },
    "minimum-stability": "stable"
}
